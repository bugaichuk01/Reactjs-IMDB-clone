{
  "compilerOptions": {
    "outDir": "./dist/",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/components/*"],
      "_/*": ["src/_helpers/*", "src/_hooks/*", "src/_services/*"]
    },
    "allowSyntheticDefaultImports": true
  }
}